ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me

# Optionally supply multiple credentials as JSON (username to password map)
# ADMIN_CREDENTIALS='{"admin":"change-me","garv":"secret"}'

# Where to read/write config files (relative to the backend folder)
CONFIG_DIRECTORY=../public/config

# GitHub commit details (required if you want GitHub commits)
GITHUB_TOKEN=ghp_your_token_here
GITHUB_OWNER=your-github-user
GITHUB_REPO=your-repo-name
GITHUB_BRANCH=main
